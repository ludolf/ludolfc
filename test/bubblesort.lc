inputArr := [5,4,8,2,1,9,3,7,6]

len := inputArr.size
i := 0
while i < len {
    j := 0
    while j < len - 1 {
        if inputArr[j] > inputArr[j + 1] {
            tmp := inputArr[j]
            inputArr[j] := inputArr[j + 1]
            inputArr[j + 1] := tmp
        }
        j := j + 1
    }
    i := i + 1
}

